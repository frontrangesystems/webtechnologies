@model IEnumerable<FrontRangeSystems.WebTechnologies.Web.Models.OrganizationModel>

@{
  ViewBag.Title = "Organizations";
}

<h1>@ViewBag.Title</h1>
@Html.Partial("_MvcNav")
<p>
  @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped">
  <tr>
    <th>
      @Html.DisplayNameFor(model => model.Name)
    </th>
    <th></th>
  </tr>

  @foreach (var item in Model)
  {
    <tr>
      <td>
        @Html.DisplayFor(modelItem => item.Name)
      </td>
      <td>
        @Html.ActionLink("Edit", "Edit", new {id = item.OrganizationId}) |
        @Html.ActionLink("Details", "Details", new {id = item.OrganizationId}) |
        @Html.ActionLink("Delete", "Delete", new {id = item.OrganizationId})
      </td>
    </tr>
  }

</table>